<svg viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
    <!-- Background circle -->
    <circle cx="200" cy="150" r="120" fill="#f8fafc" stroke="#e2e8f0" stroke-width="2" opacity="0.6"/>

    <!-- Main empty box -->
    <rect x="150" y="110" width="100" height="80" rx="8" fill="none" stroke="#cbd5e1" stroke-width="2" stroke-dasharray="8,4"/>

    <!-- Small floating elements -->
    <circle cx="120" cy="80" r="4" fill="#e2e8f0" opacity="0.7"/>
    <circle cx="280" cy="90" r="3" fill="#e2e8f0" opacity="0.5"/>
    <circle cx="130" cy="220" r="5" fill="#e2e8f0" opacity="0.6"/>
    <circle cx="290" cy="210" r="4" fill="#e2e8f0" opacity="0.4"/>

    <!-- Magnifying glass -->
    <g transform="translate(160, 120)">
        <circle cx="15" cy="15" r="12" fill="none" stroke="#94a3b8" stroke-width="3"/>
        <line x1="25" y1="25" x2="35" y2="35" stroke="#94a3b8" stroke-width="3" stroke-linecap="round"/>
    </g>

    <!-- Document icon -->
    <g transform="translate(210, 125)">
        <rect x="0" y="0" width="20" height="25" rx="2" fill="none" stroke="#94a3b8" stroke-width="2"/>
        <line x1="4" y1="8" x2="16" y2="8" stroke="#cbd5e1" stroke-width="1.5" stroke-linecap="round"/>
        <line x1="4" y1="13" x2="12" y2="13" stroke="#cbd5e1" stroke-width="1.5" stroke-linecap="round"/>
        <line x1="4" y1="18" x2="14" y2="18" stroke="#cbd5e1" stroke-width="1.5" stroke-linecap="round"/>
    </g>

    <!-- Sad face with subtle expression -->
    <g transform="translate(185, 145)">
        <!-- Face -->
        <circle cx="15" cy="15" r="20" fill="none" stroke="#94a3b8" stroke-width="2"/>
        <!-- Eyes -->
        <circle cx="10" cy="10" r="2" fill="#94a3b8"/>
        <circle cx="20" cy="10" r="2" fill="#94a3b8"/>
        <!-- Mouth -->
        <path d="M 8 22 Q 15 18 22 22" stroke="#94a3b8" stroke-width="2" fill="none" stroke-linecap="round"/>
    </g>

    <!-- Decorative dots pattern -->
    <g opacity="0.3">
        <circle cx="100" cy="120" r="1.5" fill="#cbd5e1"/>
        <circle cx="108" cy="115" r="1" fill="#cbd5e1"/>
        <circle cx="105" cy="128" r="1.2" fill="#cbd5e1"/>

        <circle cx="300" cy="160" r="1.5" fill="#cbd5e1"/>
        <circle cx="308" cy="155" r="1" fill="#cbd5e1"/>
        <circle cx="295" cy="168" r="1.2" fill="#cbd5e1"/>
    </g>

    <!-- Floating question mark -->
    <g transform="translate(250, 70)" opacity="0.6">
        <circle cx="8" cy="8" r="12" fill="#f1f5f9" stroke="#e2e8f0" stroke-width="1"/>
        <text x="8" y="13" text-anchor="middle" font-family="system-ui, -apple-system, sans-serif" font-size="14" font-weight="600" fill="#64748b">?</text>
    </g>

    <!-- Main text -->
    <text x="200" y="240" text-anchor="middle" font-family="system-ui, -apple-system, sans-serif" font-size="18" font-weight="600" fill="#475569">Nothing here yet</text>
    <text x="200" y="260" text-anchor="middle" font-family="system-ui, -apple-system, sans-serif" font-size="14" fill="#64748b">Looks like this space is waiting for content</text>

    <!-- Subtle animation for floating elements -->
    <style>
        @keyframes float {
        0%, 100% { transform: translateY(0px); }
        50% { transform: translateY(-6px); }
        }

        @keyframes fadeInOut {
        0%, 100% { opacity: 0.3; }
        50% { opacity: 0.7; }
        }

        circle:nth-child(3) { animation: float 3s ease-in-out infinite; }
        circle:nth-child(4) { animation: float 3s ease-in-out infinite 0.5s; }
        circle:nth-child(5) { animation: float 3s ease-in-out infinite 1s; }
        circle:nth-child(6) { animation: float 3s ease-in-out infinite 1.5s; }

        g:last-of-type { animation: fadeInOut 4s ease-in-out infinite; }
    </style>
</svg>